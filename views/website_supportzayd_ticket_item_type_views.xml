<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="website_supportzayd_ticket_type_view_form" model="ir.ui.view">
        <field name="name">website.supportzayd.ticket.item.type.view.form</field>
        <field name="model">website.supportzayd.ticket.item.type</field>
        <field name="arch" type="xml">
            <form string="Type of Equipment">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

      <record id="website_supportzayd_ticket_type_view_tree" model="ir.ui.view">
          <field name="name">website.supportzayd.ticket.item.type.tree.view</field>
          <field name="model">website.supportzayd.ticket.item.type</field>
          <field name="arch" type="xml">
      	      <tree editable="top">
      	          <field name="name"/>
      	      </tree>
      	  </field>
      </record>
      <record id="website_supportzayd_ticket_item_type_action" model="ir.actions.act_window">
          <field name="name">Type of Equipments</field>
          <field name="res_model">website.supportzayd.ticket.item.type</field>
          <field name="view_mode">tree,form</field>
          <field name="help" type="html">
	      <p>
	          No type of equipment found
	      </p>
          </field>
      </record>
    </data>
</odoo>
