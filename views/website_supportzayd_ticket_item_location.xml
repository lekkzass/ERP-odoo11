<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="website_supportzayd_ticket_item_location_view_form" model="ir.ui.view">
        <field name="name">website.supportzayd.ticket.item.location.view.form</field>
        <field name="model">website.supportzayd.ticket.item.location</field>
        <field name="arch" type="xml">
            <form string="Location">
                <sheet>
                    <group>
      	                <field name="name"/>

                    </group>
                </sheet>
            </form>
        </field>
    </record>

      <record id="website_supportzayd_ticket_item_location_view_tree" model="ir.ui.view">
          <field name="name">website.supportzayd.ticket.item.location.tree.view</field>
          <field name="model">website.supportzayd.ticket.item.location</field>
          <field name="arch" type="xml">
      	      <tree edit="false" >
                  <field name="name"/>

      	      </tree>
      	  </field>
      </record>

      <record id="website_supportzayd_ticket_item_location_action" model="ir.actions.act_window">
          <field name="name">List of Equipment's location</field>
          <field name="res_model">website.supportzayd.ticket.item.location</field>
          <field name="view_mode">tree,form</field>
          <field name="help" type="html">
	      <p>
	          No Equipment's location found
	      </p>
          </field>
      </record>


    </data>
</odoo>
